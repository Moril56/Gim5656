#include <stdio.h>

int main(void) {

double a = 3, b = 4; //Вхідні дані
double c, p; //Гіпотенуза та периметр 

//знаходження гіпотенузи за теореиою Піфагрора
c = sqrt(a*a + b*b);

//знаходження периметра
p = a + b + c;

//виведеня даних
printf("c = %.2f\n", c );
printf("p = %.2f\n", p );

  return 0;
}
